{
  "name": "react-todo-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "nyc --all --require babel-register mocha --require src/test/init.js --require ignore-styles src/**/*.spec.js",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.json": [
      "eslint --ext .json --fix",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": false,
    "lines": 40,
    "statements": 40,
    "functions": 20,
    "branches": 40,
    "exclude": [
      "**/*.spec.js",
      "**/*.style.js",
      "**/test*",
      "**/*config*",
      "build/*",
      "src/common/replace-module-paths.js"
    ],
    "include": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "antd": "2.13.7",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "bluebird": "^3.4.6",
    "can-use-dom": "^0.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "lazysizes": "^4.0.0-rc3",
    "lodash": "^4.17.4",
    "nodemailer": "^4.1.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react-async-script": "^0.9.1",
    "react-click-outside": "^2.2.0",
    "react-feature-toggles": "^1.1.3",
    "react-google-recaptcha": "^0.9.9",
    "react-helmet": "^5.0.2",
    "react-highlighter": "^0.3.4",
    "react-redux": "^5.0.1",
    "react-resize-detector": "^0.5.0",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.7",
    "react-slick": "^0.14.11",
    "react-sticky": "^6.0.1",
    "react-transition-group": "^1.1.2",
    "redux": "^3.6.0",
    "redux-analytics": "^0.3.1",
    "redux-polyglot": "0.4.0",
    "serialize-javascript": "^1.3.0",
    "slick-carousel": "^1.6.0",
    "smoothscroll-polyfill": "^0.3.6",
    "styled-components": "^1.4.5",
    "styled-components-theme": "^1.0.4",
    "styled-tools": "^0.1.4",
    "superagent": "^3.3.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-rewrite": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
