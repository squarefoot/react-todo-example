    "react": "^15.6.2",
    "react-dfp": "^0.6.0",
    "react-dom": "^15.6.2",
    "react-test-renderer": "15.5.4",
    "redux-test-utils": "^0.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "supertest": "^2.0.1",
    "svg-inline-loader": "^0.7.1",
