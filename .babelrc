{
  "plugins": [
    "lodash",
    "transform-decorators-legacy",
    "transform-runtime",
    "add-module-exports",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    ["module-rewrite", { "replaceFunc": "./src/common/replace-module-paths.js" }]
  ],
  "presets": [
    "react",
    "es2015",
    "stage-0",
    "stage-1",
    "es2017"
  ]
}
